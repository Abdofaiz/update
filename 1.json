{
  "Version": "1.0",
  "ReleaseNotes": "Custom configuration file",
  "Servers": [
    {
      "Name": "My SSH Server",
      "FLAG": "ðŸš€",
      "ServerIP": "your-ssh-server.com",
      "ProxyIP": "proxy-server.com",
      "ProxyPort": "8080",
      "XUser": "username",
      "XPass": "password",
      "servermessage": "Connect via SSH direct",
      "sInfo": "Speed: Fast"
    }
  ],
  "Networks": [
    {
      "SSH": [
        {
          "Name": "Direct Connection",
          "Payload": "CONNECT [host_port] [protocol][crlf][crlf]",
          "isDirect": true,
          "isSSL": false,
          "isPayloadSSL": false,
          "isSSLRp": false,
          "isSlow": false
        },
        {
          "Name": "HTTP Proxy",
          "Payload": "CONNECT [host_port] HTTP/1.0[crlf]Host: [host][crlf]X-Online-Host: [host][crlf][crlf]",
          "isDirect": false,
          "isSSL": false,
          "isPayloadSSL": false,
          "isSSLRp": false,
          "isSlow": false
        }
      ],
      "SSL": [
        {
          "Name": "SSL Connection",
          "SNI": "example.com",
          "isDirect": false,
          "isSSL": true,
          "isPayloadSSL": false,
          "isSSLRp": false,
          "isSlow": false
        },
        {
          "Name": "SSL with Payload",
          "Payload": "GET / HTTP/1.1[crlf]Host: [host][crlf]Upgrade: websocket[crlf][crlf]",
          "SNI": "cdn.example.com",
          "isDirect": false,
          "isSSL": false,
          "isPayloadSSL": true,
          "isSSLRp": false,
          "isSlow": false
        }
      ]
    }
  ]
}
